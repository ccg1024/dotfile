{
    "suggest.floatConfig": {
        "border": true
    },
    "diagnostic.floatConfig": {
        "border": true
    },
    "hover.floatConfig": {
        "border": true
    },
    "signature.floatConfig": {
        "border": true
    },
    "python.pythonPath": "~/miniforge3/envs/pytorchVenv/bin/python",
    "python.venvPath": "${env:HOME}/miniforge3/envs/",
    "coc.preferences.extensionUpdateCheck": "daily",
    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    "diagnostic.displayByAle": false,
    "diagnostic.refreshOnInsertMode": false,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.virtualText": true,
    "codeLens.enable": true,
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "coc.preferences.formatOnSaveFiletypes": [
        "json"
    ],
    "snippets.ultisnips.directories": [
        "UltiSnips",
        "$HOME/.vim/Ultisnips"
    ],
    "suggest.completionItemKindLabels": {
        "class": "\uf0e8",
        "color": "\ue22b",
        "constant": "\uf8fe",
        "default": "\uf29c",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "event": "\ufacd",
        "field": "\uf93d",
        "file": "\uf723",
        "folder": "\uf115",
        "function": "\u0192",
        "interface": "\uf417",
        "keyword": "\uf1de",
        "method": "\uf6a6",
        "module": "\uf40d",
        "operator": "\uf915",
        "property": "\ue624",
        "reference": "\ufa46",
        "snippet": "\ue60b",
        "struct": "\ufb44",
        "text": "\ue612",
        "typeParameter": "\uf728",
        "unit": "\uf475",
        "value": "\uf89f",
        "variable": "\ue71b"
    },
    "diagnostic.errorSign": "\uf467",
    "diagnostic.warningSign": "\uf071",
    "diagnostic.infoSign": "\uf129",
    "diagnostic.hintSign": "\uf864",
    "python.formatting.provider": "yapf",
    "python.formatting.yapfArgs": [
        "--style",
        "{SPACES_AROUND_POWER_OPERATOR: True, SPACES_BEFORE_COMMENT: 1}"
    ],
    "python.linting.pylintArgs": [
        "--extension-pkg-whitelist=numpy,torch,np"
    ],
    "python.analysis.typeCheckingMode": "off",
    "explorer.width": 30,
    "explorer.file.column.indent.indentLine": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.icon.source": "nvim-web-devicons",
    "explorer.file.git.showUntrackedFiles": true,
    "explorer.file.cdCommand": "cd",
    "explorer.openAction.relativePath": true,
    "colors.filetypes": [
        "*"
    ],
    "coc.source.vimtex.filetypes": [
        "tex",
        "plaintex",
        "latex"
    ]
}
